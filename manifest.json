{
    "name": "artistic new tab",
    "manifest_version": 2,
    "version": "1.1",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "browser_action": {
        "default_title": "New Tab"
    },
    "permissions": [
        "chrome://favicon/*",
        "contextMenus",
        "storage",
        "topSites"
    ],
    "background": {
        "scripts": ["interface.js", "options.js", "thirdParty/jquery-2.1.3.min.js", "spectrum.js", "thirdParty/bootstrap-3.3.4/js/bootstrap.min.js", "thirdParty/rippler/js/jquery.rippler.min.js"]
    },
    "content_scripts": [{
        "js": ["thirdParty/jquery-2.1.3.min.js", "spectrum.js", "options.js", "thirdParty/bootstrap-3.3.4/js/bootstrap.min.js", "thirdParty/rippler/js/jquery.rippler.min.js"],
        "css": ["spectrum.css", "thirdParty/bootstrap-3.3.4/css/bootstrap.min.css", "thirdParty/bootstrap-3.3.4/css/bootstrap-theme.min.css", "thirdParty/rippler/css/rippler.min.css"],
        "matches": ["file:///options.html"]
    }],
    "options_page": "options.html",
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'; connect-src 'self' http://photo.yangjunrui.com"
}
